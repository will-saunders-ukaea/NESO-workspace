<?xml version="1.0" encoding="utf-8" ?>
<NEKTAR xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.nektar.info/schema/nektar.xsd">

    <EXPANSIONS>
        <E COMPOSITE="C[1]" NUMMODES="2" FIELDS="u" TYPE="MODIFIED" />
    </EXPANSIONS>

    <CONDITIONS>

        <SOLVERINFO>
            <I PROPERTY="EQTYPE" VALUE="Poisson" />
            <I PROPERTY="Projection" VALUE="DisContinuous" />
        </SOLVERINFO>

        <PARAMETERS>
            <P> Lambda = 0.0 </P>
        </PARAMETERS>

        <VARIABLES>
            <V ID="0"> u </V>
        </VARIABLES>

        <BOUNDARYREGIONS>
            <B ID="0"> C[100] </B>
            <B ID="1"> C[200]  </B>
            <B ID="2"> C[300] </B>
            <B ID="3"> C[400] </B>
        </BOUNDARYREGIONS>

        <BOUNDARYCONDITIONS>
            <REGION REF="0">
                <N VAR="u" VALUE="0.0" />
            </REGION>
            <REGION REF="1">
                <D VAR="u" VALUE="0.0" />
            </REGION>
            <REGION REF="2">
                <N VAR="u" VALUE="0.0" />
            </REGION>
            <REGION REF="3">
                <D VAR="u" VALUE="(2*y*y*y)-(3*y*y)+1" />
            </REGION>
        </BOUNDARYCONDITIONS>

        <FUNCTION NAME="Forcing">
            <E VAR="u" VALUE="-2*((2*y*y*y)-(3*y*y)+1)+6*((1-(x*x))*((2*y)-1))" />
        </FUNCTION>

        <FUNCTION NAME="ExactSolution">
            <E VAR="u" VALUE="(1-(x*x))*((2*y*y*y)-(3*y*y)+1)" />
        </FUNCTION>

    </CONDITIONS>

</NEKTAR>
